{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=data/takeaway.db"
  },
  "OrderThrottling": {
    "MaxOrdersPerSlot": 20
  },
  "SpeechServices": {
    "SpeechToTextBaseUrl": "http://localhost:8000/",
    "TextToSpeechBaseUrl": "http://localhost:5000/"
  },
  "IntentClassifier": {
    "Enabled": true,
    "MinimumConfidence": 0.25
  }
}
